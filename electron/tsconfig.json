{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "useUnknownInCatchVariables": true,
    "sourceMap": false,
    "outDir": "../public",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["."],
  "exclude": ["__tests__"]
}
